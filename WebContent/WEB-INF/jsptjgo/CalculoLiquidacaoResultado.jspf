					<td align="center"> <%=comutacao.getDataRequisitoTemporalComutacao() %></td>
					<td align="center"> <%=comutacao.getHouveComutacao() %></td>
				<% } %>			
				</tr>
			<%}%>
				</tbody>
			</table>

		<%} else {%>
			<table><tr><td>
				<%=map.get("msgComutacao").toString()%> 
			</td></tr></table> 
		<%}%>
