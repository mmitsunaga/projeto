				<tr><td align="right">Fração exigida do crime hediondo:</td><td><%=comutacao.getDescricaoFracaoHediondo()%> (a-m-d)</td></tr>
				<% } %>
				<tr><td></td><td></td></tr>
				<tr><td align="right">Tempo cumprido total até data do decreto:</td><td><%=comutacao.getTempoCumpridoTotalAteDecretoAnos()%> (a-m-d)</td></tr>
